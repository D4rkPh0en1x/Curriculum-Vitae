{%  extends 'base.html.twig' %}



{% block body %}
<div class="datacontainer bg-secondary">
{% for row in maintext %}
      <div class="col-sm">
        <div class="card">
            <div class="card-header">
        		<h5 class="card-title text-center">{{ row.label }}</h5>
      		</div>
    		<div class="card-body">
            
            <p class="card-text">
            	{{ row.description|raw }}
            </p>
          </div>
        </div>
      </div>


{% endfor %}	
</div>

{% endblock %}